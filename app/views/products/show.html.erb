
<div class="screen-wrap">
  <div>
  <br>
   <% flash.each do |key, value| %>
        <div class="alert alert-<%= key %>"><%= value %></div>
      <% end %>
    
    
  </div>
  <br>
  <div class="table-container">
    <table class="content-table">
      <thead>
      <tr class="active-row">
        <th >ASIN</th>
        <th >Rank</th>
        <th >Category</th>
        <th >Dimensions</th>
      </tr>
      </thead>
      <tr class="active-row">
          <td ><%= @product.asin %></td>
          <td ><%= @product.rank %></td>
          <td ><%= @product.category %></td>
          <td ><%= @product.dimensions %></td>
        </tr>
    </table>
  </div>
</div>
<%= link_to "Home", products_url, class: 'button' %>


